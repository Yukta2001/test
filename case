CASE WHEN RELATION_NAME = 'AUNT(MOTHERS SISTER)' THEN 'Aunt'
WHEN RELATION_NAME = 'AUNT(WIFE OF FATHERS YOUNGER BROTHER)' THEN 'Aunt'
WHEN RELATION_NAME = 'Aunt(Fathers Sister)' THEN 'Aunt'
WHEN RELATION_NAME = 'AUNT(FATHERS SISTER)' THEN 'Aunt'
WHEN RELATION_NAME = 'Aunty' THEN 'Aunt'
WHEN RELATION_NAME = 'AUNT(WIFE OF FATHERS ELDER BROTHER)' THEN 'Aunt'
WHEN RELATION_NAME = 'AUNT(WIFE OF MOTHERS BROTHER)' THEN 'Aunt'
WHEN RELATION_NAME = 'HUSBAND OF WIFES SISTER' THEN 'Brother'
WHEN RELATION_NAME = 'Brother' THEN 'Brother'
WHEN RELATION_NAME = 'BROTHER' THEN 'Brother'
WHEN RELATION_NAME = 'HUSBAND OF HUSBANDS SISTER' THEN 'Brother'
WHEN RELATION_NAME = 'HUSBANDS YOUNGER BROTHER' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'Sisters Husband' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'WIFES BROTHER' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'SISTERS HUSBAND' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'BROTHER IN LAW' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'HUSBANDS ELDER BROTHER' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'Brother In Law' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'Brothers  Wife' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'daughter' THEN 'Daughter'
WHEN RELATION_NAME = 'Daughter' THEN 'Daughter'
WHEN RELATION_NAME = 'DAUGHTER' THEN 'Daughter'
WHEN RELATION_NAME = 'Daughter In Law' THEN 'Daughter-In-Law'
WHEN RELATION_NAME = 'DAUGHTER IN LAW' THEN 'Daughter-In-Law'
WHEN RELATION_NAME = 'Daughter in-law' THEN 'Daughter-In-Law'
WHEN RELATION_NAME = 'Father' THEN 'Father'
WHEN RELATION_NAME = 'FATHER' THEN 'Father'
WHEN RELATION_NAME = 'FATHER OF CHILDS SPOUSE' THEN 'Brother-In-Law'
WHEN RELATION_NAME = 'Father In Law' THEN 'Father-In-Law'
WHEN RELATION_NAME = 'FATHER IN LAW' THEN 'Father-In-Law'
WHEN RELATION_NAME = 'GRAND DAUGHTER' THEN 'Grand Daughter'
WHEN RELATION_NAME = 'Grand Daughter' THEN 'Grand Daughter'
WHEN RELATION_NAME = 'Grandfather(Fathers Side)' THEN 'Grand Father'
WHEN RELATION_NAME = 'Grand Father' THEN 'Grand Father'
WHEN RELATION_NAME = 'GRANDFATHER(FATHERS SIDE)' THEN 'Grand Father'
WHEN RELATION_NAME = 'GRANDFATHER(MOTHERS SIDE)' THEN 'Grand Father'
WHEN RELATION_NAME = 'Grand Mother(Fathers Side)' THEN 'Grand Mother'
WHEN RELATION_NAME = 'GRAND MOTHER(FATHERS SIDE)' THEN 'Grand Mother'
WHEN RELATION_NAME = 'GRAND MOTHER' THEN 'Grand Mother'
WHEN RELATION_NAME = 'GRAND MOTHER(MOTHERS SIDE)' THEN 'Grand Mother'
WHEN RELATION_NAME = 'GRAND SON' THEN 'Grand Son'
WHEN RELATION_NAME = 'Grand Son' THEN 'Grand Son'
WHEN RELATION_NAME = 'GREAT GRAND FATHER' THEN 'Great-Grand Father'
WHEN RELATION_NAME = 'GREAT-GRANDFATHER(FATHERS SIDE)' THEN 'Great-Grand Father'
WHEN RELATION_NAME = 'GREAT-GRANDFATHER(MOTHERS SIDE)' THEN 'Great-Grand Father'
WHEN RELATION_NAME = 'GREAT-GRANDMOTHER(MOTHERS SIDE)' THEN 'Great-Grand Mother'
WHEN RELATION_NAME = 'GREAT GRAND MOTHER' THEN 'Great-Grand Mother'
WHEN RELATION_NAME = 'GREAT-GRANDMOTHER(FATHERS SIDE)' THEN 'Great-Grand Mother'
WHEN RELATION_NAME = 'HUSBAND' THEN 'Husband'
WHEN RELATION_NAME = 'Husband' THEN 'Husband'
WHEN RELATION_NAME = 'MOTHER' THEN 'Mother'
WHEN RELATION_NAME = 'Mother' THEN 'Mother'
WHEN RELATION_NAME = 'MOTHER IN LAW' THEN 'Mother-In-Law'
WHEN RELATION_NAME = 'Mother In Law' THEN 'Mother-In-Law'
WHEN RELATION_NAME = 'NEPHEW(BROTHERS SON)' THEN 'Nephew'
WHEN RELATION_NAME = 'NEPHEW(SISTERS SON)' THEN 'Nephew'
WHEN RELATION_NAME = 'Nephew' THEN 'Nephew'
WHEN RELATION_NAME = 'NIECE(BROTHERS DAUGHTER)' THEN 'Niece'
WHEN RELATION_NAME = 'Niece' THEN 'Niece'
WHEN RELATION_NAME = 'NIECE(SISTERS DAUGHTER)' THEN 'Niece'
WHEN RELATION_NAME = 'Not Available' THEN 'Not Available'
WHEN RELATION_NAME = '\N' THEN 'Not Available'
WHEN RELATION_NAME = 'NOT AVAILABLE' THEN 'Not Available'
WHEN RELATION_NAME = '""' THEN 'Not Available'
WHEN RELATION_NAME = '-' THEN 'Not Available'
WHEN RELATION_NAME = ' ' THEN 'Not Available'
WHEN RELATION_NAME = '' THEN 'Others'
WHEN RELATION_NAME = 'Others' THEN 'Others'
WHEN RELATION_NAME = 'MOTHER OF CHILDS SPOUSE' THEN 'Others'
WHEN RELATION_NAME = 'OTHERS' THEN 'Others'
WHEN RELATION_NAME = 'OTHER' THEN 'Others'
WHEN RELATION_NAME = 'Family Head' THEN 'Self'
WHEN RELATION_NAME = 'SELF' THEN 'Self'
WHEN RELATION_NAME = 'Sister' THEN 'Sister'
WHEN RELATION_NAME = 'WIFE OF WIFES BROTHER' THEN 'Sister'
WHEN RELATION_NAME = 'WIFE OF HUSBANDS YOUNGER BROTHER' THEN 'Sister'
WHEN RELATION_NAME = 'WIFE OF HUSBANDS ELDER BROTHER' THEN 'Sister'
WHEN RELATION_NAME = 'SISTER' THEN 'Sister'
WHEN RELATION_NAME = 'Wife Of Husbands Elder Brother' THEN 'Sister'
WHEN RELATION_NAME = 'BROTHERS  WIFE' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'HUSBANDS SISTER' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'Husbands Sister' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'SISTER IN LAW' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'WIFES SISTER' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'Sister In Law' THEN 'Sister-In-Law'
WHEN RELATION_NAME = 'SON' THEN 'Son'
WHEN RELATION_NAME = 'Son' THEN 'Son'
WHEN RELATION_NAME = 'son' THEN 'Son'
WHEN RELATION_NAME = 'SON IN LAW' THEN 'Son-In-Law'
WHEN RELATION_NAME = 'Son in-law' THEN 'Son-In-Law'
WHEN RELATION_NAME = 'Son In Law' THEN 'Son-In-Law'
WHEN RELATION_NAME = 'UNCLE(MOTHERS BROTHER)' THEN 'Uncle'
WHEN RELATION_NAME = 'Uncle' THEN 'Uncle'
WHEN RELATION_NAME = 'UNCLE(FATHER ELDER BROTHER)' THEN 'Uncle'
WHEN RELATION_NAME = 'UNCLE(FATHERS YOUNGER BROTHER)' THEN 'Uncle'
WHEN RELATION_NAME = 'Uncle(Mothers Brother)' THEN 'Uncle'
WHEN RELATION_NAME = 'Wife' THEN 'Wife'
WHEN RELATION_NAME = 'WIFE' THEN 'Wife' END AS RELATION_NAME
